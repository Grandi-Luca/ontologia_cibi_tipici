    <rdf:Description>
        <swrla:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrla:isRuleEnabled>
        <rdfs:comment>Se un ingrediente non è compatibile con una dieta allora il cibo tipico che lo utilizza non sarà anch&apos;esso compatibile</rdfs:comment>
        <rdfs:label>Cibo tipico incompatibile con dieta</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
