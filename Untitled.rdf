    <!-- http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/CiboTipico -->

    <owl:Class rdf:about="http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/CiboTipico">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.org/ontology/fo/Food"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/tipicoDiCucina"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Cucina"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://purl.org/ontology/fo/Food"/>
        <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/PiattoLocale"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/ProdottoLocale"/>
        </owl:disjointUnionOf>
        <rdfs:comment xml:lang="it">Un cibo tipico è un alimento o piatto culinario che è intrinsecamente legato a una particolare regione geografica o tradizione culinaria. Questi cibi rappresentano l&apos;identità e la peculiarità gastronomica di una determinata area e sono spesso tramandati da generazioni, incorporando ingredienti, tecniche di preparazione e sapori caratteristici della regione di origine</rdfs:comment>
        <rdfs:label xml:lang="it">CiboTipico</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Cucina -->

    <owl:Class rdf:about="http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Cucina">
        <rdfs:comment xml:lang="it">Rappresenta la tipologia di cucina a cui un cibo tipico appartiene</rdfs:comment>
        <rdfs:label xml:lang="it">Cucina</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Dieta -->

    <owl:Class rdf:about="http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Dieta">
        <owl:equivalentClass rdf:resource="https://purl.org/ontology/fo/Diet"/>
        <rdfs:comment xml:lang="it">Una dieta è un modo di selezionare il cibo che ottiene un effetto particolare, ad esempio mangiando in modo favorevole alla gravidanza o evitando cibi che contengono crostacei.</rdfs:comment>
        <rdfs:label xml:lang="it">Dieta</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Ingrediente -->

    <owl:Class rdf:about="http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Ingrediente">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item"/>
        <rdfs:comment xml:lang="it">Combinazione di una quantità e di un alimento, un ingrediente indica la quantità di un alimento che dovrebbe essere utilizzato nella ricetta.</rdfs:comment>
        <rdfs:label xml:lang="it">Ingrediente</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/ListaIngredienti -->

    <owl:Class rdf:about="http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/ListaIngredienti">
        <owl:equivalentClass>
            <owl:Class>
