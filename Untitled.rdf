    <owl:Ontology rdf:about="http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici">
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/list.owl"/>
        <dc:creator>Luca Grandi</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-02-05T00:00:00</dc:date>
        <dc:description xml:lang="it">L&apos;ontologia riguarda i &quot;Cibi Locali&quot; e mira a rappresentare in modo strutturato e semantico le informazioni relative ai cibi, alle ricette e alle caratteristiche culinarie di specifiche zone geografiche.</dc:description>
        <dc:title xml:lang="it">Ontologia su cibi tipici locali</dc:title>
        <vann:preferredNamespacePrefix xml:lang="it">cibi_tipici</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceURI>http://www.semanticweb.org/cibi_tipici</vann:preferredNamespaceURI>
        <rdfs:label xml:lang="it">CibiTipici</rdfs:label>
    </owl:Ontology>
    


