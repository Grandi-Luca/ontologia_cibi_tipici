                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Ingrediente"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag"/>
        <rdfs:comment xml:lang="it">Insieme non ordinato di Ingredienti</rdfs:comment>
        <rdfs:label xml:lang="it">ListaIngredienti</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Occasione -->

    <owl:Class rdf:about="http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Occasione">
        <rdfs:comment xml:lang="it">Occasione nel quale un cibo tipico viene proposto. E.g. Sagra, Festa di natale, Pasqua, Capodanno, evento gastronomico, festa di compleanno ecc.</rdfs:comment>
        <rdfs:label xml:lang="it">Occasione</rdfs:label>
    </owl:Class>
    


