  owl:distinctMembers ( :BrodoVegetale
                        :Burro
                        :Cipolla
                        :FecolaPatate
                        :Latte
                        :OlioEV
                        :RisoCarnaroli
                        :Sale
                        :Sgalogno
                        :Timo
                        :UvaValliOssolane
                        :VinoBianco
                        :Zucchero
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :antipasto
                        :dolce
                        :primoPiatto
                        :secondoPiatto
                      )
] .


