@prefix : <http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/> .

<http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici> rdf:type owl:Ontology ;
                                                                owl:imports <http://www.ontologydesignpatterns.org/cp/owl/list.owl> .

#################################################################
#    Annotation properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#hasFirstItem
<http://www.ontologydesignpatterns.org/cp/owl/list.owl#hasFirstItem> rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#hasLastItem
<http://www.ontologydesignpatterns.org/cp/owl/list.owl#hasLastItem> rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows
<http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows> rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyPrecedes
<http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyPrecedes> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/appartieneA
:appartieneA rdf:type owl:ObjectProperty ;
             rdfs:domain :TipologiaCucina ;
             rdfs:range :AreaGeografica ;
             rdfs:comment "Proprietà che indica l'appartenenza della tipologia di cucina a un'area geografica specifica."@it ;
             rdfs:label "appartieneA"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/ciboHaIngrediente
:ciboHaIngrediente rdf:type owl:ObjectProperty ;
                   rdfs:domain :CiboTIpico ;
                   rdfs:range :Ingrediente ;
                   owl:propertyChainAxiom ( :haRicetta
                                            :usaListaIngredienti
                                            <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem>
                                            :contieneIngrediente
                                          ) ;
                   rdfs:comment "proprietà che associa ad un cibo tipico gli ingredienti contenuti nella lista degli ingredienti"@it ;
                   rdfs:label "ciboHaIngrediente"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/contieneIngrediente
:contieneIngrediente rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent> ;
                     rdfs:domain <https://purl.org/ontology/fo/Ingredient> ;
                     rdfs:range :Ingrediente ;
                     rdfs:comment "Proprietà che funge da collegamento alla risorsa effettiva a cui fa riferimento l’elemento"@it ;
                     rdfs:label "contieneIngrediente"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/haCertificazione
:haCertificazione rdf:type owl:ObjectProperty ;
                  rdfs:domain :CiboTIpico ;
                  rdfs:range :Certificazione ;
                  rdfs:comment "Proprietà che associa una certificazione ad un cibo tipico"@it ;
                  rdfs:label "haCertificazione"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/haRicetta
:haRicetta rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :CiboTIpico ;
           rdfs:range :Ricetta ;
           rdfs:comment "Proprietà che associa ad un cibo locale una ricetta"@it ;
           rdfs:label "haRicetta"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/haTipologia
:haTipologia rdf:type owl:ObjectProperty ;
             rdfs:domain :PiattoLocale ;
             rdfs:range :TipologiaPiatto ;
             rdfs:comment "Proprietà che associa ad un piatto locale una tipologia"@it ;
             rdfs:label "haTipologia"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/produce
:produce rdf:type owl:ObjectProperty ;
         owl:inverseOf :vieneProdottoDa ;
         rdfs:domain :Produttore ;
         rdfs:range :CiboTIpico ;
         rdfs:comment "Proprietà che associa i cibi tipici alla persona che li produce"@it ;
         rdfs:label "produce"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/propostoInOccasione
:propostoInOccasione rdf:type owl:ObjectProperty ;
                     rdfs:domain :CiboTIpico ;
                     rdfs:range :Occasione ;
                     rdfs:comment "Proprietà che associa le occasioni in cui è consigliato consumare un cibo tipico"@it ;
                     rdfs:label "propostoInOccasione"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/tipicoDiCucina
:tipicoDiCucina rdf:type owl:ObjectProperty ;
                rdfs:domain :CiboTIpico ;
                rdfs:range :TipologiaCucina ;
                rdfs:comment "Proprietà che associa un cibo tipico ad una tipologia di cucina"@it ;
                rdfs:label "tipicoDiCucina"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/usaListaIngredienti
:usaListaIngredienti rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain :Ricetta ;
                     rdfs:range :ListaIngredienti ;
                     rdfs:comment "Proprietà che associa la lista degli ingredienti utiizzati ad una ricetta"@it ;
                     rdfs:label "usaListaIngredienti"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/usaProcesso
:usaProcesso rdf:type owl:ObjectProperty ;
             rdfs:domain :Ricetta ;
             rdfs:range :ProcessoDiPreparazione ;
             rdfs:comment "Proprietà che associa alla ricetta il prorpio insieme di step per la preparazione"@it ;
             rdfs:label "usaProcesso"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/usaTecnica
:usaTecnica rdf:type owl:ObjectProperty ;
            rdfs:domain :StepPreparazione ;
            rdfs:range :TecnicaDiPreparazione ;
            rdfs:comment "Proprietà che associa ad uno specifico step le tecniche utilizzate"@it ;
            rdfs:label "usaTecnica"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/vieneProdottoDa
:vieneProdottoDa rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain :CiboTIpico ;
                 rdfs:range :Produttore ;
                 rdfs:comment "Proprietà che associa i produttori ad uno specifico cibo tipico"@it ;
                 rdfs:label "vieneProdottoDa"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/èDiStagione
:èDiStagione rdf:type owl:ObjectProperty ;
             rdfs:domain :CiboTIpico ;
             rdfs:range :Stagionalità ;
             rdfs:comment "Proprietà che specifica a quale stagione appartiene un cibo tipico"@it ;
             rdfs:label "èDiStagione"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/haDescrizione
:haDescrizione rdf:type owl:DatatypeProperty ;
               rdfs:domain :StepPreparazione ;
               rdfs:range xsd:string ;
               rdfs:comment "Descrizione dettagliata di uno step di preparazione"@it ;
               rdfs:label "haDescrizione"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/haQuantitàIngrediente
:haQuantitàIngrediente rdf:type owl:DatatypeProperty ;
                       rdfs:domain <https://purl.org/ontology/fo/Ingredient> ;
                       rdfs:comment "Numero della quantità di ingrediente necessaria per la preparazione di una ricetta. L'unità di misura è il grammo"@it ;
                       rdfs:label "haQuantitàIngrediente"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/haStoria
:haStoria rdf:type owl:DatatypeProperty ;
          rdfs:domain :CiboTIpico ;
          rdfs:range xsd:string ;
          rdfs:comment "Descrizione della storia di un cibo locale"@it ;
          rdfs:label "haStoria"@it .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/AreaGeografica
:AreaGeografica rdf:type owl:Class ;
                rdfs:comment "Rappresenta la provenienza di un cibo tipico"@it ;
                rdfs:label "AreaGeografica"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Carne
:Carne rdf:type owl:Class ;
       rdfs:subClassOf :Ingrediente ;
       rdfs:comment "Classe che rappresenta la carne"@it ;
       rdfs:label "Carne"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/CarneAnimaleDaCortile
:CarneAnimaleDaCortile rdf:type owl:Class ;
                       rdfs:subClassOf :Carne ;
                       rdfs:comment "Carne proveniente da animali da cortile come pollame"@it ;
                       rdfs:label "CarneAnimaleDaCortile"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/CarneAnimaleDaMacello
:CarneAnimaleDaMacello rdf:type owl:Class ;
                       rdfs:subClassOf :Carne ;
                       owl:disjointUnionOf ( :CarneBovina
                                             :CarneCaprina
                                             :CarneEquina
                                             :CarneOvina
                                             :CarneSuina
                                             :Selvaggina
                                           ) ;
                       rdfs:comment "Carne proveniente da animali da macello come mucche e pecore"@it ;
                       rdfs:label "CarneAnimaleDaMacello"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/CarneBovina
:CarneBovina rdf:type owl:Class ;
             rdfs:subClassOf :CarneAnimaleDaMacello ;
             rdfs:comment "Carne proveniente da animali classificati come bovini"@it ;
             rdfs:label "CarneBovina"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/CarneCaprina
:CarneCaprina rdf:type owl:Class ;
              rdfs:subClassOf :CarneAnimaleDaMacello ;
              rdfs:comment "Carne proveniente da animali classificati come caprini"@it ;
              rdfs:label "CarneCaprina"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/CarneEquina
:CarneEquina rdf:type owl:Class ;
             rdfs:subClassOf :CarneAnimaleDaMacello ;
             rdfs:comment "Carne proveniente da animali classificati come equini"@it ;
             rdfs:label "CarneEquina"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/CarneOvina
:CarneOvina rdf:type owl:Class ;
            rdfs:subClassOf :CarneAnimaleDaMacello ;
            rdfs:comment "Carne proveniente da animali classificati come ovini"@it ;
            rdfs:label "CarneOvina"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/CarneSuina
:CarneSuina rdf:type owl:Class ;
            rdfs:subClassOf :CarneAnimaleDaMacello ;
            rdfs:comment "Carne proveniente da animali classificati come suini"@it ;
            rdfs:label "CarneSuina"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Certificazione
:Certificazione rdf:type owl:Class ;
                rdfs:comment "Rappresenta le certificazioni che possono essere assegnate ai prodotti locali per garantire la loro autenticità e qualità"@it ;
                rdfs:label "Certificazione"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/CiboTIpico
:CiboTIpico rdf:type owl:Class ;
            rdfs:comment "Classe principale che rappresenta i cibi prodotti localmente."@it ;
            rdfs:label "CiboTipico"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Fungo
:Fungo rdf:type owl:Class ;
       rdfs:subClassOf :Ingrediente ;
       rdfs:comment "Classe che rappresenta i funghi"@it ;
       rdfs:label "Fungo"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Grasso
:Grasso rdf:type owl:Class ;
        rdfs:subClassOf :Ingrediente ;
        rdfs:comment "Classe che rappresenta la tipologia di grasso usata come ingrediente"@it ;
        rdfs:label "Grasso"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/GrassoAnimale
:GrassoAnimale rdf:type owl:Class ;
               rdfs:subClassOf :Grasso ;
               rdfs:comment "Grasso con origine animale"@it ;
               rdfs:label "GrassoAnimale"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/GrassoVegetale
:GrassoVegetale rdf:type owl:Class ;
                rdfs:subClassOf :Grasso ;
                rdfs:comment "Grasso con origine vegetale"@it ;
                rdfs:label "GrassoVegetale"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Ingrediente
:Ingrediente rdf:type owl:Class ;
             rdfs:comment "Ingrediente usato per preparare una ricetta"@it ;
             rdfs:label "Ingrediente"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/ItemIngrediente
:ItemIngrediente rdf:type owl:Class ;
                 owl:equivalentClass <https://purl.org/ontology/fo/Ingredient> ;
                 rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item> ;
                 rdfs:comment "Combinazione di una quantità e di un alimento, che indica la quantità di qualcosa che dovrebbe essere utilizzato nella ricetta."@it ;
                 rdfs:label "ItemIngrediente"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Latticino
:Latticino rdf:type owl:Class ;
           rdfs:subClassOf :Ingrediente ;
           rdfs:comment "Classe che rappresenta i prodotti derivati dal latte"@it ;
           rdfs:label "Latticino"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/ListaIngredienti
:ListaIngredienti rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag>
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem> ;
                                                               owl:someValuesFrom :Ingrediente
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> ;
                  rdfs:comment "Insieme non ordinato di Ingredient"@it ;
                  rdfs:label "ListaIngredienti"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Occasione
:Occasione rdf:type owl:Class ;
           owl:equivalentClass <https://purl.org/ontology/fo/Occasion> ;
           rdfs:comment "Occasione nel quale un cibo tipico viene proposto. E.g. Sagra, Festa di natale, Pasqua, Capodanno, evento gastronomico, festa di compleanno ecc."@it ;
           rdfs:label "Occasione"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Pesce
:Pesce rdf:type owl:Class ;
       rdfs:subClassOf :Ingrediente ;
       rdfs:comment "Classe che rappresenta i pesci utilizzati come ingredienti"@it ;
       rdfs:label "Pesce"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/PiattoLocale
:PiattoLocale rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :CiboTIpico
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :haTipologia ;
                                                           owl:someValuesFrom :TipologiaPiatto
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf :CiboTIpico ;
              rdfs:comment "Cibo tipico che viene servito come un valore appartenente all'enumerazione TipologiaPiatto"@it ;
              rdfs:label "PiattoLocale"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/PiattoLocaleVegetariano
:PiattoLocaleVegetariano rdf:type owl:Class ;
                         owl:equivalentClass [ owl:intersectionOf ( :PiattoLocale
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :ciboHaIngrediente ;
                                                                      owl:allValuesFrom [ rdf:type owl:Class ;
                                                                                          owl:complementOf [ rdf:type owl:Class ;
                                                                                                             owl:unionOf ( :Carne
                                                                                                                           :GrassoAnimale
                                                                                                                           :Pesce
                                                                                                                         )
                                                                                                           ]
                                                                                        ]
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] ;
                         rdfs:subClassOf :PiattoLocale ;
                         rdfs:comment "Piatto locale che non contiene né carne né pesce"@it ;
                         rdfs:label "PiattoLocaleVegetariano"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/ProcessoDiPreparazione
:ProcessoDiPreparazione rdf:type owl:Class ;
                        rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/list.owl#List> ;
                        rdfs:comment "Insieme ordinato di Step attraverso i quali è possibile realizzare una ricetta"@it ;
                        rdfs:label "ProcessoDiPreparazione"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/ProcessoDiPreparazioneLungo
:ProcessoDiPreparazioneLungo rdf:type owl:Class ;
                             owl:equivalentClass [ rdf:type owl:Restriction ;
                                                   owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#size> ;
                                                   owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                        owl:onDatatype xsd:nonNegativeInteger ;
                                                                        owl:withRestrictions ( [ xsd:minInclusive "6"^^xsd:nonNegativeInteger
                                                                                               ]
                                                                                             )
                                                                      ]
                                                 ] ;
                             rdfs:subClassOf :ProcessoDiPreparazione ;
                             rdfs:comment "Insieme ordinato di Step attraverso i quali è possibile realizzare una ricetta. Il numero di step necessari è maggiore o uguale di 6"@it ;
                             rdfs:label "ProcessoDiPreparazioneLungo"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Produttore
:Produttore rdf:type owl:Class ;
            rdfs:comment "Persona che produce un cibo tipico"@it ;
            rdfs:label "Produttore"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Ricetta
:Ricetta rdf:type owl:Class ;
         rdfs:comment "Combinazione di ListaIngredienti e ProcessoDiProduzione per realizzare un cibo tipico"@it ;
         rdfs:label "Ricetta"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Selvaggina
:Selvaggina rdf:type owl:Class ;
            rdfs:subClassOf :CarneAnimaleDaMacello ;
            rdfs:comment "Carne proveniente da animali classificati come selvaggina"@it ;
            rdfs:label "Selvaggina"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Stagionalità
:Stagionalità rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Class ;
                                    owl:oneOf ( :AnnoIntero
                                                :Autunno
                                                :Estate
                                                :Inverno
                                                :Primavera
                                              )
                                  ] ;
              rdfs:comment "Rappresenta la stagionalità dei cibi tipici"@it ;
              rdfs:label "Stagionalità"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/StepPreparazione
:StepPreparazione rdf:type owl:Class ;
                  rdfs:comment "Elemento appartenente ad un processo di preparazione"@it ;
                  rdfs:label "StepPreparazione"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/TecnicaDiPreparazione
:TecnicaDiPreparazione rdf:type owl:Class ;
                       rdfs:comment "Particolare tecnica utilizzata per lo svolgimento di uno step di preparazione"@it ;
                       rdfs:label "TecnicaDiPreparazione"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/TipologiaCucina
:TipologiaCucina rdf:type owl:Class ;
                 rdfs:comment "Tipologia di cucina"@it ;
                 rdfs:label "TipologiaCucina"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/TipologiaPiatto
:TipologiaPiatto rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:oneOf ( :Antipasto
                                                   :Contorno
                                                   :Dolce
                                                   :Primo
                                                   :Secondo
                                                 )
                                     ] ;
                 rdfs:comment "Enumerazione delle tipologie di piatto locale"@it ;
                 rdfs:label "TipologiaPiatto"@it .


###  https://purl.org/ontology/fo/Ingredient
<https://purl.org/ontology/fo/Ingredient> rdf:type owl:Class ;
                                          rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item> ;
                                          rdfs:comment "An Ingredient is the combination of a quantity and a food, giving the amount of something that should be used in the recipe."@en ;
                                          rdfs:label "Ingredient"@en .


###  https://purl.org/ontology/fo/Occasion
<https://purl.org/ontology/fo/Occasion> rdf:type owl:Class ;
                                        rdfs:comment "An Occasion is a recognised event or type of event that a particular food may be suitable for. An example of a particular occasion might be Chinese New Year, whereas a more general occasion might a birthday."@en ;
                                        rdfs:label "Occasion"@en .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/AnnoIntero
:AnnoIntero rdf:type owl:NamedIndividual ,
                     :Stagionalità ;
            rdfs:comment "Individuo che appresenta l'intero anno"@it ;
            rdfs:label "AnnoIntero"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Antipasto
:Antipasto rdf:type owl:NamedIndividual ,
                    :TipologiaPiatto ;
           rdfs:comment "Rappresenta la tipologia degli antipasti"@it ;
           rdfs:label "Antipasto"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Autunno
:Autunno rdf:type owl:NamedIndividual ,
                  :Stagionalità ;
         rdfs:comment "Indica la stagione autunnale"@it ;
         rdfs:label "Autunno"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/BottigliaValliOssolanePrunent
:BottigliaValliOssolanePrunent rdf:type owl:NamedIndividual ,
                                        :Ingrediente ;
                               rdfs:comment "Individuo che rappresenta una bottiglia valli ossolane prunent"@it ;
                               rdfs:label "BottigliaValliOssolanePrunent"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/BrodoVegetale
:BrodoVegetale rdf:type owl:NamedIndividual ,
                        :Ingrediente ;
               rdfs:comment "Individuo che rappresenta brodo vegetale"@it ;
               rdfs:label "BrodoVegetale"@it .


###  http://www.semanticweb.org/lucagrandi/ontologies/cibi_tipici/Burro
:Burro rdf:type owl:NamedIndividual ,
                :Latticino ;
       rdfs:comment "Individuo che rappresenta il burro"@it ;
       rdfs:label "Burro"@it .


