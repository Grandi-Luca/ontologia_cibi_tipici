    <!-- http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag">
        <rdfs:comment xml:lang="en">Bag - Collection that can have a number of copies of each object</rdfs:comment>
        <rdfs:label xml:lang="en">(collections) Bag</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection"/>
        <rdfs:comment xml:lang="en">Item - Element belonging to a Bag</rdfs:comment>
        <rdfs:label xml:lang="en">item</rdfs:label>
    </owl:Class>
</rdf:RDF>



